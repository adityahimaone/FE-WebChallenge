html {
  font-family: Helvetica, sans-serif;
}

.navbar-collapse {
  justify-content: end !important;
}

.color-primary-5 {
  color: #091b6f;
  background-color: #091b6f;
}

.color-primary-4 {
  color: #0d28a6;
}

.color-primary-3 {
  color: #091b6f;
}

.color-primary-2 {
  color: #aeb7e1;
}
.color-primary-1 {
  color: #cfd4ed;
}

.color-secondary-5 {
  color: #5cb85f;
  background-color: #5cb85f;
}

#hero {
  height: 504px;
  background-color: #f1f3ff;
}

.hero-left {
  position: relative;
  width: 50%;
}

.hero-right {
  position: relative;
  width: 50%;
  height: 100%;
}

.hero-shape {
  position: absolute;
  width: 704px;
  height: 243px;
  right: -110px;
  bottom: 0px;
  background: #0d28a6;
  border-radius: 60px 0px 0px 0px;
  z-index: 0;
}

.hero-img {
  position: absolute;
  z-index: 1;
  right: -120px;
  bottom: 40px;
}

#our-service {
  background-color: #fff;
}

.service-item {
  height: 428px;
  padding: 60px 40px;
}

.service-img {
  position: absolute;
  width: 374px;
  height: 366px;
  left: 99px;
  top: 0px;
  z-index: 5;
}

.service-left {
  position: relative;
  width: 50%;
}

.service-right {
  width: 50%;
}

.service-right > div {
  margin-top: 150px;
}

.service-shape-blue {
  position: absolute;
  width: 360px;
  height: 360px;
  left: 99px;
  top: 68px;
  border-radius: 50%;
  /* Primary/DarkBlue/04 */
  background: #0d28a6;
}

.service-shape-green {
  position: absolute;
  width: 133px;
  height: 133px;
  left: 0px;
  top: 1px;
  border-radius: 50%;
  /* Primary/LimeGreen/03 */
  background: #92d094;
}

.service-shape-yellow {
  position: absolute;
  width: 48px;
  height: 48px;
  left: 60px;
  top: 336px;
  border-radius: 50%;
  /* Allert/Warning */
  background: #f9cc00;
}

.service-shape-red {
  position: absolute;
  width: 25px;
  height: 25px;
  left: 383px;
  top: 43px;
  border-radius: 50%;
  /* Allert/Danger */

  background: #fa2c5a;
}
